[
  {
    "path": "/content/test/composum/authtest",
    "acl": [
      {
        "principal": "everyone",
        "acl": [
          {
            "allow": false,
            "privileges": "jcr:all"
          }
        ]
      },
      {
        "principal": "composum-platform-auth-external",
        "groupPath": "composum/platform",
        "acl": [
          {
            "allow": true,
            "privileges": "jcr:all"
          }
        ]
      }
    ]
  },
  {
    "path": "/home/users/keycloak",
    "jcr:primaryType": "rep:AuthorizableFolder",
    "acl": [
      {
        "principal": "composum-platform-auth-service",
        "acl": [
          {
            "allow": true,
            "privileges": "jcr:all"
          }
        ]
      }
    ]
  },
  {
    "path": "/home/groups",
    "acl": [
      {
        "principal": "composum-platform-auth-service",
        "acl": [
          {
            "allow": true,
            "privileges": [
              "jcr:read",
              "jcr:modifyProperties",
              "jcr:modifyAccessControl"
            ]
          }
        ]
      }
    ]
  },
  {
    "path": "/conf/composum/platform/auth",
    "acl": [
      {
        "principal": "composum-platform-auth-service",
        "acl": [
          {
            "allow": true,
            "privileges": [
              "jcr:read"
            ]
          }
        ]
      }
    ]
  }
]
